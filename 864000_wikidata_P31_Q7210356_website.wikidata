SELECT DISTINCT ?item ?itemLabel ?website WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language "[AUTO_LANGUAGE]". }
  {
    SELECT DISTINCT ?item ?itemLabel ?website WHERE {
      ?item p:P31 ?statement0.
      ?item wdt:P856 ?website .
      ?statement0 (ps:P31/(wdt:P279*)) wd:Q7210356.
    }
  }
}
